    "@yext/chat-ui-react": "^0.7.2",
    "@yext/components-tsx-maps": "^1.0.11",
    "@yext/search-headless-react": "^2.3.0",
    "@yext/search-ui-react": "^1.3.0",
    "@yext/sites-components": "^1.0.0-rc.4",
    "class-variance-authority": "^0.7.0",
